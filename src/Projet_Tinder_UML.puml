@startuml
left to right direction

enum Genre {
    Homme
    Femme
}

'--- Classes ---'
class Profil {
    id : entier
    nom : chaîne
    age : entier
    genre : chaîne
    ville : chaîne
    quartier : chaîne
    textePresentation : chaîne
    centresInterets : liste chaîne
    photos : liste images
    interets : HashMap Profil,booléen
    estBloque : booléen
    notifMail : booléen
    agePrefere : entier
    genrePrefere : chaîne
    ProximiteGeograhpique: chaîne
    bloqueConversation : HashMap Profil,Profil
    Profil(nom : chaîne, age : entier, genre : chaîne, ville : chaîne,
    quartier : chaîne, presentation : chaîne, interets : liste chaîne)
    mettreAJourPreferences(age : entier, genre : chaîne, lieu : chaîne)
    signalerProfil(user : Profil)
    signalerMessage(message : Message)
    envoyerMessage(destinataire : Profil, contenu : chaîne)
    mettreAJourInfos(presentation : chaîne)
    setEstBloque()
    getNotifMail() : booléen
    setNotifMail()
    modifierMessage(message : Message, contenu : chaîne)
    supprimerMessage(message : Message)
    supprimerNotification(notification : Notification)
    possibleMatch(binome : Profil) : booléen
    peutEnvoyerMessage(destinataire : Profil) : booléen
    ajoutInteresser(user : Profil)
    rechercher(chaîne... profils) : liste Profil
}

class Message {
    contenu : chaîne
    getContenu() : chaîne
    getExpediteur() : Profil
    getDestinataire() : Profil
    setContenu(contenu : chaîne)
}

class Notification {
    contenu : chaîne
    envoi()
    getContenu() : chaîne
    getDestinataire() : Profil
    setContenu(contenu : chaîne)
}

class Match {
    Match(user1 : Profil, user2 : Profil)
    getUser1() : Profil
    getUser2() : Profil
}

class Signalement {
    id : entier
    raisonSignalement : chaîne
    Signalement(user : Profil)
    Signalement(user : Profil, message : Message)
}

class Moderateur {
    id : entier
    nom : chaîne
    Moderateur(nom : chaîne)
    verifierSignalement(signalement : Signalement)
    bloquerProfil(profil : Profil)
    communiquer(profil : Profil)
    verifierInscription(profil : Compte) : booléen
}


class Compte {
    pseudo : chaîne
    email : chaîne
    motDePasse : chaîne
    document : image
    inscrire(email : chaîne, pseudo : chaîne, mdp : chaîne)
    connecter(psuedo : chaîne, email : chaîne, mdp : chaîne) : booléen
    verifierDocument(doc : image) : booléen
    reinitialiserMotDePasse(newMdp : chaîne)
}

enum TypeNotif{
    Message
    Interet
    Match
    Profil
}


'--- Relations ---'
Profil "2" -- "0..*" Message : conversation
Profil "2" -- "0..*" Match : intéressé
Profil "1" -- "0..*" Signalement : victime
Profil "1" <-- "0..*" Signalement : fautif
Moderateur "1" --> "0..*" Signalement : verifieSignalement
Message "0..1" <-- "1" Signalement : messageOffensant
Profil "1" -- "0..*" Notification : reçoit
Profil "1" -- "1" Genre : identifieComme
Compte "0..*" <-- "1" Moderateur : verifierDocument
Compte "0..1" -- "1" Profil : permetLaConnexion
Notification "1"--"1" TypeNotif : type
'(Classe pour une liste de profils recommandés)'
Profil "0..*"--"0..*" Profil : recommendations

@enduml